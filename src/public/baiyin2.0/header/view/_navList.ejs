<script type="text/html" id="navList">
    <%
      if( typeof curIndex == 'undefined' ){
        curIndex = 0;
      }
    %>
    {{each navList}}
      {{if $value.childNav}}
        <li class="resourceNav{{$index == <%= curIndex %> ? ' nav_act' : ''}}">
          <a href="javascript:void(0)" class="nav_li">{{$value.name}}<span class="arrow"></span></a>
          <div class="chooseList resource clearfix">
            <ul class="clearfix">
              {{each $value.childNav}}
                <li class="list"><a href="{{$value.url}}">{{$value.name}}</a></li>
              {{/each}}
            </ul>
          </div>
      {{else}}
        <li>
          <a href="{{$value.url}}" class="nav_li{{$index == <%= curIndex %> ? ' nav_act' : ''}}{{ $value.type ? ' verify_app_enter' : ''}}"  type="{{$value.type}}"  {{$value.newOpen=='true' ? 'target=_blank' : ''}}>{{$value.name}}</a>
      {{/if}}
      </li>
    {{/each}}
  </script>