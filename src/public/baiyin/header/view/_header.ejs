<link rel="stylesheet" href="../../../public/header/css/header.css">
<div class="wrap_top_bg baiyin">
  <div class="wrap_top">
    <div class="logo">
      <a href="javascript:void(0)"><img src="" alt=""></a>
    </div>
    <div class="chooseArea tabchoose">
      <a class="area" href="javascript:;">
        <span class="kh">[</span>
        <span class="scholl_name">白银区</span>
        <span class="arrow arrowDown"></span>
        <span class="kh">]</span>
      </a>
      <div class="chooseList schoolChoose">
        <ul id="schoolList"></ul>
        <script type="text/html" id="school_list">
          {{each data}}
            {{if $index%4 == 0 }}
            <li class="list clearfix">
            {{/if}}
              <span data-id="{{$value.id}}"><a href="javascript:;" title="{{$value.name}}">{{$value.showName}}</a></span>
            {{if ($index+1)%4 == 0}}
            </li>
            {{/if}}
          {{/each}}
        </script>
      </div>
    </div>
    <div id="nav" class="nav clearfix">
      <div class="nav_top">
        <div class="msg tabchoose">
          <div class="msgT clearfix">
            <ul id="login_message" class="nav_top_ul">
              <li class="favorite_button">添加收藏</li><li>|</li>
              <li class="home_button">设为首页</li>
            </ul>
            <script type="text/html" id="loginMessage">
              <li>|</li>
              <li class="userName">
                {{if data.code == "login_error" }}
                  <span class="loginButton">登录</span>
                {{else}}
                  <span>{{data.data.userInfo.name}}&nbsp;<span class="arrow arrowDown"></span></span>

                  <div class="chooseList myChoose clearfix">
                    <ul class="clearfix">
                    {{if data.data.userType == 1 }}
                      <li><a href="{{data.userJumpHref.spaceManager}}">机构空间</a></li>
                      <li><a href="{{data.userJumpHref.myApp}}">我的应用</a></li>
                    {{else}}
                      <li><a href="{{data.userJumpHref.mySpace}}">我的空间</a></li>
                      <li><a href="{{data.userJumpHref.myApp}}">我的应用</a></li>
                      <li><a href="{{data.userJumpHref.personCenter}}">个人中心</a></li>
                    {{/if}}
                      <li><a class="logout" href="javascript:void(0)">退出</a></li>
                    </ul>
                  </div>
                {{/if}}
              </li>
              {{if data.code != "login_error" && data.data.userType == 1 }}
                <li>|</li>
                <li><a href="{{data.userJumpHref.adminManager}}" target="_blank">后台管理</a></li>
              {{/if}}
              <!-- <li>|</li>
               <li>消息(<b>5</b>)</li>-->
            </script>
          </div>
        </div>
      </div>
      <div class="nav_bottom">
        <ul class="clearfix"></ul>
        <script type="text/html" id="navList">
          <%
            if( typeof curIndex == 'undefined' ){
              curIndex = 0;
            }
          %>
          {{each navList}}
            {{if $value.childNav}}
              <li class="resourceNav{{$index == <%= curIndex %> ? ' nav_act' : ''}}">
                <a href="javascript:void(0)" class="nav_li">{{$value.name}}<span class="arrow"></span></a>
                <div class="chooseList resource clearfix">
                  <ul class="clearfix">
                    {{each $value.childNav}}
                      <li class="list"><a href="{{$value.url}}">{{$value.name}}</a></li>
                    {{/each}}
                  </ul>
                </div>
            {{else}}
              <li>
                <a href="{{$value.url}}" class="nav_li{{$index == <%= curIndex %> ? ' nav_act' : ''}}{{ $value.type ? ' verify_app_enter' : ''}}"  type="{{$value.type}}"  {{$value.newOpen=='true' ? 'target=_blank' : ''}}>{{$value.name}}</a>
            {{/if}}
          </li>
          {{/each}}
        </script>
      </div>
    </div>
  </div>
</div>
