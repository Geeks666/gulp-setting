<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge">
  <meta name="renderer" content="webkit|ie-comp|ie-stand">
  <title>白银区智慧教育云中心</title>
  <link rel="stylesheet" type="text/css" href="../../../lib/layer/skin/layer.css" />
  <link rel="stylesheet" href="../home/css/common.css">
  <link rel="stylesheet" href="../home/css/index.css">
  <link rel="stylesheet" href="../home/css/album.css">
  <script>
    if(top.location != self.location){
      top.location = self.location;//防止页面被框架包含
    }
  </script>
</head>
<body>
  <div class="wrapper">
    <%- include('../../../public/baiyin2.0/header/view/_header.ejs') %>
    <%- include('../home/view/_index_banner.ejs') %>
    <div class="ann clearfix wrap" style="margin-top: 17px;">
      <ul class="notice_list"></ul>
      <script id="noticeList" type="text/html">
        {{each data}}
          <li>
            <span class="msgIcon"></span>
            <a class="annLink" href="../notice/noticeDetail.html?id={{$value.id}}&index={{$index}}">{{$value.title}}</a>
            <a class="more" href="../notice/noticeList.html?currentPage=1" target="_blank">更多</a>
          </li>
        {{/each}}
      </script>
    </div>
    <div class="newsAndResult">
      <div class="pictureBlock">
        <a href="javascript:void(0);"><img src="images/default_news.jpg" alt=""></a>
        <p class="imgTitle"></p>
        <ul class="imgtab"></ul>
      </div>
      <div class="newsAndResultBlock">
        <h2 class="tabNewsAndResult">
          <a href="javascript:void(0);" class="active">教育新闻</a><a href="javascript:void(0);">新闻纵横</a>
          <a class="overMore" href="../news/sitenewsListInCategory.html">更多</a>
        </h2>
        <ul class="news" id="newsShowBox">
        </ul>
        <script id="newsShowBox_" type="text/html">
          {{each data}}
          <li class="news-item"><a class="con" href="../news/sitenewsDetail.html?id={{$value
          .id}}&index={{$index}}&category={{category}}{{$value.orgid}}" title="{{$value.title}}">{{$value.title}}</a><span class="time">{{$value
              .crtDttm.substring(0,10)}}</span></li>
          {{/each}}
        </script>
        <ul class="news" id="resultShowBox">
        </ul>
        <script id="resultShowBox_" type="text/html">
          {{each data}}
          <li class="news-item"><a class="con" href="../news/sitenewsDetail.html?id={{$value
          .id}}&index={{$index}}&category={{category}}{{$value.orgid}}" title="{{$value.title}}">{{$value.title}}</a><span class="time">{{$value.crtDttm.substring(0,10)}}</span></li>
          {{/each}}
        </script>
      </div>
    </div>

    <div class="searchBox">
      <div class="clearfix wrap">
        <div class="search">
           <input class="searchText" placeholder="他们正在搜：一元二次方程" type="text">
           <div class="searchBtn_wrap">
             <span>资源搜索</span>
             <a class="searchBtn" target="_blank" href="javascript:void(0)"></a>
           </div>
        </div>
        <a id="activityRegister" class="apply"  href="javascript:void(0);"></a>
        <a id="reviewEnter" class="entrance" href="javascript:;" data-href="/activity/www/background/"></a>
      </div>
    </div>
    <div id="videoClass">
      <div class="videContent wrap clearfix">
        <script type="text/html" id="subjectSection">
          {{each data as phase i}}
            <div class="{{gradeClass[phase.name]}}">
              <h3 class="schoolTitle">{{phase.name}}</h3>
              <div class="schoolMenu" style="{{i==0 ? 'display: block' : ''}}">
                <div class="menuList">
                  {{each phase.subjects }}
                        <a class="list" href="{{msg}}/syncTeaching/index/?subjectId={{$value.id}}&gradeId={{phase.phaseId}}&requireLogin=false">{{$value.name}}</a>
                  {{/each}}
                </div>
              </div>
            </div>
          {{/each}}
        </script>
        <div class="videoListWrap">
          <h2 class="title">
            课堂实录
            <a class="more verify_app_enter" type="hanbovideo_appId" href="javascript:;">更多</a>
          </h2>
          <div class="videoListCont">
            <ul id="videoList" class="videoList"></ul>
            <script type="text/html" id="videoList_">
              {{each data.result.data}}
                <li class="list">
                  <a class="videoPic" href="{{$value.redirectUrl}}" target="_blank">
                    <img src="{{$value.imageUrl}}" alt="">
                    {{if false}}
                    <span class="icon"></span>
                    {{/if}}
                  </a>
                  <p><a class="videoTitle" href="{{$value.redirectUrl}}" title="{{$value.title}}">{{$value.title}}</a></p>
                  <p class="clearfix">
                    <span class="author">{{$value.user_name}}</span>
                    <span class="showNum"><span>{{$value.count}}</span>浏览</span>
                  </p>
                </li>
              {{/each}}
            </script>
          </div>
        </div>
        <div class="adImageWrap">
          <a href="javascript:;" class="gotoApps icon-resource"target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-yk" target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-activity" target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-studio" target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-ep" target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-volunteer" target="_blank"></a>
          <a href="javascript:;" class="gotoApps icon-rank" target="_blank"></a>
        </div>
      </div>
    </div>
    <div class="wrap apps_Wrap">
      <div id="apps">
        <ul class="clearfix">
          <li class="applist" id="jxds">
            <a target="_blank" href="javascript:void(0)" data-href="http://zl.feitianedu.com:8088/">
               <span class="icon icon-jxds">
               </span>
              <span class="appName" style="color: #0dd1bc;">教学大师</span>
            </a>
          </li>
          <li class="applist" id="ktsl">
            <a target="_blank" href="javascript:void(0)" type="hanbovideo_appId" class="verify_app_enter">
               <span class="icon icon-ktsl">
               </span>
              <span class="appName" style="color: #ffae00;">课堂实录</span>
            </a>
          </li>
          <li class="applist" id="hdjy" >
            <a target="_blank" href="javascript:void(0)" type="teachingresearch_appId" class="verify_app_enter">
               <span class="icon icon-hdjy">
               </span>
              <span class="appName" style="color: #11b75d;">互动教研</span>
            </a>
          </li>
          <li class="applist" id="zygl">
            <a target="_blank" href="javascript:void(0)" type="resource_appId" class="verify_app_enter">
               <span class="icon icon-zygl">
               </span>
              <span class="appName" style="color: #2ea4ff;">资源管理</span>
            </a>
          </li>
          <li class="applist" id="sztsg">
            <a target="_blank" href="javascript:void(0)" type="ebook_appId" class="verify_app_enter">
               <span class="icon icon-sztsg">
               </span>
              <span class="appName" style="color: #f15e4c;">数字图书馆</span>
            </a>
          </li>
          <li class="applist" id="applistMore">
            <a target="_blank" href="javascript:void(0)">
               <span class="icon icon-gd">
               </span>
              <span class="appName" style="color: #5078fa;">更多</span>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <!--校园风采-->
    <div class="schoolBlock">
      <h2 class="news-title wrap">校园风采</h2>
      <div class="schoolBlockBox">
        <div class="schoolList" id="schoolShow">
          <ul class="schoolListBox">
            <li class="schoolList-item">
              <a href="javascript:void(0);" class="schoolPic"><img src="images/default_news.jpg" alt=""></a>
              <p class="desc">nihaola</p>
            </li>
          </ul>
        </div>
        <script id="schoolShow_" type="text/html">
          {{each data}}
          <li class="schoolList-item" data-num="{{$value.num}}">
            <a href="javascript:void(0);" class="schoolPic"><img src="{{$value.picture_src}}" alt=""></a>
            <p class="desc">{{$value.org_name}}</p>
          </li>
          {{/each}}
        </script>
        <a class="prev tabBtn" href="javascript:;"></a>
        <a class="next tabBtn" href="javascript:;"></a>
      </div>
    </div>
    <div id="teacherRank" class="teacher_ranked">
      <div class="wrap content clearFix">
        <div class="tabList">
          <h3 class="clearFix">
            <div class="teacherNum titleMsg">登录人数：<span class="num">0</span></div>
            <div class="loginNum titleMsg share_resource_num">分享资源数：<span class="num">0</span></div>
            <a href="javascript:;" id="teacherRanked" class="gotoRank" target="_blank">进入排行榜</a>
          </h3>
          <div class="box">
            <div class="listContent" id="teaSort">
              <div></div>
            </div>
            <script type="text/html" id="teaSort_">
              {{each data}}
                {{ if $index == 0 || $index%10 == 0 }}
                  <ul class="listPage clearfix">
                {{/if}}
                <li class="list">
                  <div class="number">{{$index+1}}</div>
                  <p class="schoolName">
                    <a href="javascript:;">{{$value.flago}}</a>
                    <span class="num">{{$value.flags}}</span>
                  </p>
                  <p class="msg clearFix"><span class="num">{{$value.resUpCount}}</span>资源</p>
                  <p class="msg clearFix"><span class="num">{{$value.videoCount}}</span>视频</p>
                </li>
                {{ if ($index != 0 && ($index+1)%10 == 0) || $index == data.length-1 }}
                    </ul>
                {{/if}}
              {{/each}}
            </script>
            <a class="prev tabBtn" href="javascript:;"></a>
            <a class="next tabBtn" href="javascript:;"></a>
          </div>
        </div>
      </div>
    </div>
    <!--教师的照片墙-->
    <div id="pic_show" class="clearfix">
      <ul id="Album"><li class="album_big"><span href="javascript:;" id="you_logo">教师空间</span></li></ul>

      <script id="Album_" type="text/html">
        {{each data}}
        <li>
          <a href="{{$value.spaceUrl}}" target="_blank"><em></em><img src="{{$value.icon}}" /></a>
          <div class="Album_info"><strong>{{$value.name}}</strong><span>{{$value.label}}</span></div>
        </li>
        {{/each}}</script>
        <!--<li>
          <a href="http://www.17sucai.com/"><em></em><img src="images/teacher_default.jpg" /></a>
          <div class="Album_info"><strong>颜经理</strong><span>销售/贸易经理</span></div>
        </li>-->



    </div>
    <div class="wrap friends" >
      <div>
        <h2><b></b>友情链接</h2>
        <ul id="link_content" class="list"></ul>
        <script type="text/html" id="linkContent">
          {{each data}}
            <li><a href="{{$value.website}}" target="_blank"><img src="{{$value.pic}}"/></a></li>
          {{/each}}
        </script>
      </div>
    </div>
    <ul id="others">
      <li id="goTop">
        <span class="icon"></span>
        返回顶部
      </li>
    </ul>
    <%- include('../../../public/baiyin/footer/view/_footer.ejs') %>
  </div>
  <div class="mask"></div>
  <div class="picDialog">
    <div class="closeDialog"></div>
    <div class="picImgWrap">
      <div class="picImgLeft"><span></span></div>
      <div class="picImgRight"><span></span></div>
      <div class="picImg">
        <ul id="picImg" class="pictureList1"></ul>
        <script type="text/html" id="pictureList1">
          {{each data}}
          <li data-num="{{$value.num}}">
            <img src="{{$value.picture_src ? $value.picture_src : 'images/2.gif'}}" alt="">
            <span title="{{$value.name}}">{{$value.org_name}}</span>
          </li>
          {{/each}}
        </script>
      </div>
    </div>
  </div>
  <script>
    function imgError(obj,src) {
      obj.onerror=null;
      obj.src=src;
    }
  </script>
  <script src="../../../lib/requires/requires.js" data-main="../home/js/index.js"></script>
</body>
</html>