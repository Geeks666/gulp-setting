<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <meta charset="UTF-8">
  <title>数据监控系统</title>
  <link rel="stylesheet" href="../../customModule/statistics/css/main.css">

</head>
<body>
<div class="bg-gradient">
  <%- include('view/_header.ejs') %>
  <div class="wrap">
    <%- include('view/_sidebar.ejs', {currentPage : 'user'}) %>
    <div class="content">
      <%- include('view/_user_top.ejs', {currentPage : 'userparticipation'}) %>
      <div class="row row2-user-participation echart">
        <div class="echart-mark-top-right"></div>
        <div class="echart-title">用户使用趋势</div>
        <div class="dataWrap">
          <input placeholder="请选择开始日期" readonly class="laydate-icon" id="date_start"/>
          <input placeholder="请选择结束日期" readonly class="laydate-icon" id="date_end"/>
          <input type="button" value="确定" class="dateConfirmBtn">
        </div>
        <div id="user_trend"></div>
      </div>
      <div class="row row2-user-participation echart no-margin">
        <div class="echart-mark-top-right"></div>
        <div class="echart-title">用户使用频率</div>
        <div class="dataWrap">
          <input placeholder="请选择日期" readonly class="laydate-icon" id="date_signle">
        </div>
        <div id="user_frequency"></div>
      </div>
    </div>
    <%- include('view/_footer.ejs') %>
  </div>
</div>
<script src="../../../../lib/requires/requires.js"
        data-main="../../customModule/statistics/js/user_participation.js"></script>
<script type="text/javascript" src="../../../../lib/component/laydate/js/laydate.js"></script>
<script>
  laydate.skin('shenlan');
</script>
</body>
</html>