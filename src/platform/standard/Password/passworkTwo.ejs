<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
  <meta name="renderer" content="webkit|ie-stand"/>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="Pragma" content="no-cache"/>
  <meta http-equiv="Cache-Control" content="no-cache, must-revalidate"/>
  <meta http-equiv="Expires" content="0"/>
  <title>找回密码</title>
  <link rel="stylesheet" type="text/css" href="../Password/css/password.css">
</head>
<body>
<div class="passWordHeader">
  <div class="headerWrap">
    <div class="logo"></div>
  </div>
</div>
<div class="pW_cont">
  <div class="PW_cont_c">
    <div class="pW_bg pW_l">
      <dl class="actbg">
        <dd>1</dd>
        <dt>验证身份</dt>
      </dl>
    </div>
    <div class="act_border"></div>
    <div class="pW_bg pW_c">
      <dl class="actbg">
        <dd>2</dd>
        <dt>找回密码</dt>
      </dl>
    </div>
    <div class="border"></div>
    <div class="pW_bg pW_r">
      <dl>
        <dd>3</dd>
        <dt>设置成功</dt>
      </dl>
    </div>

  </div>
  <div class="PW_form">
    <form action="">
      <p class="promptInfo pwdInfo">
        <span class="icon"></span>
        <span class="info"></span>
      </p>
      <p>
        <label for="">新密码：</label><input type="password" class="pwd" id="pwd">
        <strong>请输入6-10位密码（由大写A-Z，小写a-z,数字0-9组成）</strong>
      </p>
      <p>
        <label for="">确认新密码：</label><input type="password" class="pwd1" id="pwd1">
        <strong>请输入6-10位密码（由大写A-Z，小写a-z,数字0-9组成）</strong>
      </p>
      <p>
        <label for=""></label><input type="button" class="nextStepBtn" id="nextStepBtn2" value="下一步">
      </p>
    </form>
  </div>
</div>
<%- include('../../../public/standard/footer/view/_footer.ejs') %>
<script src="../../../lib/requires/requires.js" data-main="../Password/js/password.js"></script>
</body>
</html>
